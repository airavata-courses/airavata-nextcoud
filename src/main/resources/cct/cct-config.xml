  <archivers type="">
    <archiver name="Z archiver"   desc="Z archiver" ext="Z" className=""/>
    <archiver name="gz archiver"   desc="gz archiver" ext="gz" className=""/>
    <archiver name="jar archiver"   desc="jar archiver" ext="jar" className=""/>
    <archiver name="bz2Z archiver"   desc="bz2 archiver" ext="bz2" className=""/>
    <archiver name="zip archiver"   desc="zip archiver" ext="zip" className=""/>
  </archivers>
  
  <menuBar className="cct.dialogs.JEditorFrame">
    <menu>
      
    </menu>
  </menuBar>
