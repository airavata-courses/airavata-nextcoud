<root>
  <formats type="">
    <format name="allFormats"   desc="All Chemistry Format Files" ext="*" className=""/>
    <format name="adfInput"     desc="Amsterdam Density Functional package (ADF) Files" ext="in;inp;dat;run;Z;gz;zip;jar;bz2" 
            parser="cct.adf.ADF" className=""/>
    <format name="adfOutput"    desc="ADF Output" ext="out;log" className=""/>
    <format name="g03Input"     desc="Gaussian G03 Input" ext="gjf;com;inp;dat;in;g03" parser="cct.gaussian.Gaussian" className=""/>
    <format name="g03Output"    desc="Gaussian G03 Output" ext="log;out" parser="cct.gaussian.GaussianOutput" className=""/>
