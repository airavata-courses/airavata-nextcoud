    <format name="mdlMol"       desc="MDL Molfile Format" ext="mol;sd;sdf" className=""/>
    <format name="xmolXYZ"      desc="XMol XYZ Format" ext="xyz" className=""/>
    <format name="gromacsGRO"    desc="Gromacs Coordinate Format" ext="gro" className=""/>
    <format name="amberPrmtop"  desc="Amber prmtop Format" ext="top;prmtop" className=""/>
    <format name="amberPrep"    desc="Amber PREP Format" ext="in;prepin;prep" className=""/>
